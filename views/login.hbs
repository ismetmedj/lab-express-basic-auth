<h1>Log In</h1>
{{#if errorMessage}}
  <p>{{errorMessage}}</p>
{{/if}}

<form action="/login" method="post">
  <label for="username">Username :</label>
  <input type="text" name="username" />

  <label for="password">Password :</label>
  <input type="password" name="password" />
  <button>Log In</button>

</form>