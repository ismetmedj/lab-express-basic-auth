<h1>Sign Up</h1>
{{#if errorMessage}}
  <p>{{errorMessage}}</p>
{{/if}}
<form action="/signup" method="post">
  <label for="username">Username :</label>
  <input type="text" name="username" />

  <label for="password">Password :</label>
  <input type="password" name="password" />
  <button>Sign Up</button>

</form>